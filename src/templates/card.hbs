<li class="gallery-item">
        <div class="photo-card">
          <img data-img="{{largeImageURL}}" src="{{webformatURL}}" width="{{webformatWidth}}" alt="" height="{{webformatHeight}}" />
          <div class="stats">
            <p class="stats-item">
              <i class="material-icons">thumb_up</i>&nbsp;
              {{likes}}
            </p>
            <p class="stats-item">
              <i class="material-icons">visibility</i>&nbsp;
              {{views}}
            </p>
            <p class="stats-item">
              <i class="material-icons">comment</i>&nbsp;
              {{comments}}
            </p>
            <p class="stats-item">
              <i class="material-icons">cloud_download</i>&nbsp;
              {{downloads}}
            </p>
          </div>
        </div>
      </li>